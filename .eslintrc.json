{
    "env": {
        "es6": true,
        "es2017": true,
        "es2020": true,
        "node": false,
        "browser": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
      "no-alert": "warn",
      "no-eval": "error",
      "require-await": "warn",
      "strict": "error"
    }
}
